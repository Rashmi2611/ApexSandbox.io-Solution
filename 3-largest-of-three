public static Integer findLargest(Integer num1, Integer num2, Integer num3) {
	if(num1>num2 && num1>num3){
        system.debug('num1 is largest no' + num1);
        return num1;
    }
   else if(num2>num3){
      system.debug('num2 is largest no' + num2);
        return num2; 
   }
   else{
       return num3;
   }
}